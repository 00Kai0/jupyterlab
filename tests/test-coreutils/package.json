    "@phosphor/signaling": "^1.2.2",
    "chai": "~4.1.2",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.4"
  },
  "devDependencies": {
    "@types/chai": "~4.0.10",
    "@types/jest": "^23.3.1",
    "rimraf": "~2.6.2",
    "typescript": "~3.1.1"
  }
}
